.tou{
  position: fixed;
  top: 0;
  width: 100vw;
height: 100%;
background: linear-gradient(0deg,#F6F6F6 58%, #E5F1FF 100%);
display: flex;
z-index: -1;
}
.xiang{
  width: 88px;
height: 88px;
border-radius: 50%;
/* background: rgba(20, 119, 255, 0.23); */
display: flex;
align-items: center;
justify-content: center;
margin-left: 22px;
margin-top: 14vh;
}
.xiang image{
  width: 100px;
height: 100px;
}
.msg{
  margin-left: 12px;
margin-top: 14vh;
}
.name{
  margin-top: 18px;
  font-family: Alibaba PuHuiTi 3.0;
  font-size: 18px;
  font-weight: 600;
  line-height: 24px;
  display: flex;
  align-items: center;
  letter-spacing: -0.3px;
  color: #6D6D6D;
}
.num{
  margin-top: 8px;
  /* margin-left: 4px; */
  font-family: Alibaba PuHuiTi 3.0;
  font-size: 14px;
  font-weight: normal;
  line-height: 24px;
  display: flex;
  align-items: center;
  letter-spacing: -0.3px;
  font-variation-settings: "opsz" auto;
  color: #828282;
}
.headtext {
  font-family: Alibaba PuHuiTi 3.0;
  font-size: 16px;
  font-weight: 600;
  line-height: 24px;
  text-align: center;
  display: flex;
  align-items: center;
  letter-spacing: -0.3px;
  /* 颜色/中性色色板/#OAOAOA */
  color: #0A0A0A;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 6.6vh;
}

.det{
  position: fixed;
  bottom: 0;
  border-radius: 20px;
opacity: 1;
background: #FAFAFE;
width: 100vw;
height: 74vh;
}
.detitem{
  margin-left: 4%;
  margin-top: 21vh;
  width: 92%;
  opacity: 1;
}
.item{
  border-radius: 8px;
opacity: 1;
background: #FFFFFF;
height: 48px;
  font-family: Source Han Sans;
  font-size: 16px;
  font-weight: normal;
  line-height: normal;
  letter-spacing: 0em;
  font-variation-settings: "opsz" auto;
  color: #3D3D3D;
display: flex;
justify-content: space-between;
align-items: center;
margin-bottom: 10px;
padding-left: 10px;
padding-right: 15px;
}
.jjj{
  width: 24px;
  height: 24px;
}
.zzz image{
  height: 20px;
  width: 20px;
  margin-right: 10px;
  margin-left: 3px;
}
.zzz{
  display: flex;
  align-items: center;
  justify-content: center;
  font-family: Alibaba PuHuiTi 3.0;
font-size: 16px;
font-weight: 500;
line-height: 24px;
display: flex;
align-items: center;
letter-spacing: -0.3px;
font-variation-settings: "opsz" auto;
color: #3D3D3D;
}
.containers{
  position: relative;
  z-index: 1;
}